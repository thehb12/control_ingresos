		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{# {% block no_body %} #}

		{% block body %}

			{{ include('navbar.html.twig') }}
			{{ include('sidebar.html.twig') }}
			<main id="main" class="main">
				<div class="row">
					<div class="col-3 mb-3 offset-md-9"></div>
				</div>

