					<div class="copyright">
						&copy; Unidad Clinica San Nicolas LTDA. Todos los derechos reservados.
					</div>
					<div class="credits">Software desarrollado por
						<a href=>Julian Hernandez</a>
					</div>
				</footer>
				<!-- End Footer -->

			{% endblock %}
			<a href="#" class="back-to-top d-flex align-items-center justify-content-center">
				<i class="bi bi-arrow-up-short"></i>
			</a>


